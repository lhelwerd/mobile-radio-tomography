{
    "distance_sensor_simulator": {
        "name": "Distance sensor (simulator)",
        "settings": {
            "altitude_margin": 2.5,
            "angle_margin": 5.0,
            "maximum_distance": 1000.0
        }
    },
    "distance_sensor_physical": {
        "name": "Distance sensor (physical)",
        "settings": {
            "trigger_pin": 13,
            "echo_pin": 11,
            "trigger_delay": 0.00001,
            "interval_delay": 0.5,
            "speed_of_sound": 34320
        }
    },
    "infrared_sensor": {
        "name": "Infrared sensor",
        "settings": {
            "remote": "Sony_RM-SRB5",
            "program": "mobile-radio-tomography",
            "buttons": ["start", "stop", "up", "down", "left", "right"],
            "wait_delay": 0.05
        }
    },
    "line_follower_base": {
        "name": "Line follower base",
        "settings": {
            "readable_leds": [0, 2, 3, 5],
            "line_threshold": 300,
            "line_follower_module": "Line_Follower_Arduino"
        }
    },
    "line_follower_raspberry_pi": {
        "name": "Line follower (Raspberry Pi)",
        "parent": "line_follower_base",
        "settings": {
            "led_pins": [29, 31, 32, 33, 35, 36],
            "emitter_pin": 37,
            "write_delay": 0.0002,
            "max_value": 1023,
            "charge_delay": 0.00001
        }
    },
    "line_follower_arduino": {
        "name": "Line follower (Arduino)",
        "parent": "line_follower_base",
        "settings": {
            "serial_device": "/dev/ttyUSB1",
            "serial_baud_rate": 9600,
            "serial_flow_control": false
        }
    },
    "mission": {
        "name": "Drone trajectory mission data",
        "settings": {
            "altitude_undershoot": 0.95,
            "closeness": 2.0,
            "farness": 100.0,
            "mission_delay": 2.0,
            "altitude": 4.0,
            "padding": 4.0,
            "speed": 2.5,
            "space_size": 100.0,
            "resolution": 1,
            "yaw_step": 10.0,
            "mission_class": "Mission_Browse"
        }
    },
    "mission_monitor": {
        "name": "Mission monitoring",
        "settings": {
            "step_delay": 0.5,
            "plot": true,
            "viewer": true
        }
    },
    "environment": {
        "name": "Vehicle environment",
        "settings": {
            "geometry_class": "Geometry_Spherical",
            "distance_sensors": [0.0],
            "servo_pins": [
                {
                    "pin": 7,
                    "angles": [0,360],
                    "pwm": [1000,2000]
                }
            ],
            "xbee_type": "",
            "infrared_sensor": true,
            "scenefile": null,
            "translation": [0.0,0.0,0.0],
            "location_check": false
        }
    },
    "environment_viewer": {
        "name": "Environment 3D scene viewer",
        "settings": {
            "max_points": 200
        }
    },
    "environment_viewer_interactive": {
        "name": "Interactive environment 3D scene viewer",
        "parent": "environment_viewer",
        "settings": {
            "camera_speed": 5.0,
            "rotate_speed": 90.0
        }
    },
    "vehicle": {
        "name": "Vehicle interface",
        "settings": {
            "vehicle_class": "Mock_Vehicle"
        }
    },
    "vehicle_dronekit": {
        "name": "Dronekit vehicle backend",
        "settings": {
            "connect": "127.0.0.1:14550",
            "mavlink_baud_rate": 115200,
            "vehicle_simulation": true,
            "gps": true
        }
    },
    "vehicle_robot": {
        "name": "Base robot vehicle",
        "settings": {
            "diverged_speed": 0.2,
            "diverged_time": 0.2,
            "rotate_speed": 0.30,
            "vehicle_delay": 0.05,
            "line_follower_delay": 0.01,
            "home_location": [0,0],
            "home_direction": 0
        }
    },
    "vehicle_robot_raspberry_pi": {
        "name": "Robot vehicle directly connected to a Raspberry Pi",
        "settings": {
            "direction_pins": [16,18],
            "speed_pins": [24,26],
            "speed_pwms": [0, 255],
            "speeds": [0, 0.60]
        }
    },
    "vehicle_robot_arduino": {
        "name": "Robot vehicle connected through an Arduino with a serial connection",
        "settings": {
            "speed_pwms": [-400, 400],
            "speeds": [-0.60, 0.60]
        }
    },
    "reconstruction": {
        "name": "Reconstruction",
        "settings": {
            "filename": "static",
            "reconstructor": "truncated-svd",
            "pause_time": 0.75
        }
    },
    "reconstruction_viewer": {
        "name": "Reconstruction viewer",
        "settings": {
            "cmap": "pink",
            "interpolation": "hamming"
        }
    },
    "reconstruction_weight_matrix": {
        "name": "Reconstruction weight matrix",
        "settings": {
            "distance_lambda": 0.3
        }
    },
    "reconstruction_truncated_svd_reconstructor": {
        "name": "Reconstruction truncated SVD reconstructor",
        "settings": {
            "singular_values": 3
        }
    },
    "xbee_base": {
        "name": "XBee base",
        "settings": {
            "number_of_sensors": 2,
            "custom_packet_limit": 1,
            "loop_delay": 0.01,
            "baud_rate": 57600,
            "specifications": {
                "memory_map_chunk": [
                    {
                        "name": "id",
                        "format": "B",
                        "value": 1,
                        "private": false
                    },
                    {
                        "name": "latitude",
                        "format": "d"
                    },
                    {
                        "name": "longitude",
                        "format": "d"
                    }
                ],
                "rssi_broadcast": [
                    {
                        "name": "id",
                        "format": "B",
                        "value": 2,
                        "private": true
                    },
                    {
                        "name": "latitude",
                        "format": "d"
                    },
                    {
                        "name": "longitude",
                        "format": "d"
                    },
                    {
                        "name": "valid",
                        "format": "?"
                    },
                    {
                        "name": "sensor_id",
                        "format": "B"
                    },
                    {
                        "name": "timestamp",
                        "format": "d"
                    }
                ],
                "rssi_ground_station": [
                    {
                        "name": "id",
                        "format": "B",
                        "value": 3,
                        "private": true
                    },
                    {
                        "name": "from_latitude",
                        "format": "d"
                    },
                    {
                        "name": "from_longitude",
                        "format": "d"
                    },
                    {
                        "name": "from_valid",
                        "format": "?"
                    },
                    {
                        "name": "to_latitude",
                        "format": "d"
                    },
                    {
                        "name": "to_longitude",
                        "format": "d"
                    },
                    {
                        "name": "to_valid",
                        "format": "?"
                    },
                    {
                        "name": "rssi",
                        "format": "B"
                    }
                ],
                "ntp": [
                    {
                        "name": "id",
                        "format": "B",
                        "value": 4,
                        "private": true
                    },
                    {
                        "name": "sensor_id",
                        "format": "B"
                    },
                    {
                        "name": "timestamp_1",
                        "format": "d"
                    },
                    {
                        "name": "timestamp_2",
                        "format": "d"
                    },
                    {
                        "name": "timestamp_3",
                        "format": "d"
                    },
                    {
                        "name": "timestamp_4",
                        "format": "d"
                    }
                ]
            }
        }
    },
    "xbee_configurator": {
        "name": "XBee configurator",
        "parent": "xbee_base",
        "settings": {
            "port": "/dev/ttyUSB0",
            "pan_id": 2015,
            "startup_delay": 1
        }
    },
    "xbee_sensor_simulator": {
        "name": "XBee sensor (simulator)",
        "parent": "xbee_base",
        "settings": {
            "ip": "127.0.0.1",
            "port": 3233,
            "buffer_size": 256,
            "xbee_id": 0
        }
    },
    "xbee_sensor_physical": {
        "name": "XBee sensor (physical)",
        "parent": "xbee_base",
        "settings": {
            "port": "/dev/ttyUSB0",
            "sensors": [
                "\\x00\\x13\\xa2\\x00@\\xe6n\\xbd",
                "\\x00\\x13\\xa2\\x00@\\xe6o5",
                "\\x00\\x13\\xa2\\x00@\\xe6n\\xb9"
            ],
            "ntp_delay": 3,
            "response_delay": 0.3,
            "startup_delay": 1,
            "synchronize": false
        }
    },
    "xbee_tdma_scheduler": {
        "name": "XBee TDMA scheduler",
        "parent": "xbee_base",
        "settings": {
            "sweep_delay": 0.5
        }
    }
}
